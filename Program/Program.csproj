<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="BitcoinLib" Version="1.15.0" />
    <PackageReference Include="HDWallet.Core" Version="0.11.0" />
    <PackageReference Include="NBitcoin" Version="7.0.40" />
    <PackageReference Include="Nethereum.HdWallet" Version="4.26.0" />
    <PackageReference Include="Nethereum.JsonRpc.RpcClient" Version="4.26.0" />
    <PackageReference Include="Nethereum.Web3" Version="4.26.0" />
    <PackageReference Include="RestShark" Version="1.0.0" />
    <PackageReference Include="RestSharp" Version="112.1.0" />
  </ItemGroup>

</Project>